<!DOCTYPE html>
<html>
<head>
  <title>Auto</title>
  <%= stylesheet_link_tag    "application", media: "all" %>
  <%= javascript_include_tag "application"%>
  <%= csrf_meta_tags %>
</head>
<body>
<% if flash[:notice] %>
    <div class="alert alert-dismissable alert-success" style="top: 0;position: absolute;width: 100%;z-index: 9999999;">
      <button type="button" class="close" data-dismiss="alert">×</button>
      <%= flash[:notice] %>
    </div>
<% end %>
<% if flash[:alert] %>
    <div class="alert alert-dismissable alert-danger" style="top: 0;position: absolute;width: 100%;z-index: 9999999;">
      <button type="button" class="close" data-dismiss="alert">×</button>
      <%= flash[:alert] %>
    </div>

<% end %>
<%= render 'layouts/header'%>

<%= yield %>

</body>
</html>
